{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["dom", "es6"],
    "jsx": "react",
    "module": "ES6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src", "firebaseConfig.js", "src/global.d.ts", "src/**/*.scss", "public"]
}
